{
  "name": "electron-printer",
  "description": "Node.js printer bindings -  electron builds",
  "version": "0.0.7",
  "homepage": "https://github.com/marcopiraccini/electron-printer",
  "author": {
    "name": "Marco Piraccini",
    "email": "marco.piraccini@gmail.com"
  },
  "contributors": [
    "Name Lastname <email@server.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/marcopiraccini/electron-printer.git"
  },
  "engines": {
    "electron": ">=1.4.10"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "release-linux-ia32": "node-pre-gyp rebuild --target_arch=ia32 --dist-url=https://atom.io/download/atom-shell --target_platform=linux --runtime=electron --target=1.4.10 && node-pre-gyp --target_arch=ia32 --target_platform=linux --runtime=electron --target=1.4.10 package && node-pre-gyp-github publish --release",
    "release-linux": "node-pre-gyp rebuild --target_arch=x64 --dist-url=https://atom.io/download/atom-shell --runtime=electron --target=1.4.10 --target_platform=linux && node-pre-gyp package --target_arch=x64 --target_platform=linux --runtime=electron --target=1.4.10",
    "release-win-ia32": "node-pre-gyp rebuild --target_arch=ia32 --target_platform=win32 --runtime=electron --target=1.4.10 --dist-url=https://atom.io/download/atom-shell && node-pre-gyp package --target_arch=ia32 --target_platform=win32 --runtime=electron --target=1.4.10",
    "release-win": "node-pre-gyp rebuild --target_arch=x64 --target_platform=win32 --runtime=electron --target=1.4.10 --dist-url=https://atom.io/download/atom-shell && node-pre-gyp package --target_arch=x64 --target_platform=win32 --runtime=electron --target=1.4.10",
    "release": "node-pre-gyp rebuild --runtime=electron --target=1.4.10 --dist-url=https://atom.io/download/atom-shell && node-pre-gyp package --runtime=electron --target=1.4.10",
    "release-darwin": "HOME=~/.electron-gyp node-pre-gyp rebuild --target_platform=darwin --runtime=electron --target=1.4.10 --arch=x64 --dist-url=https://atom.io/download/atom-shell && node-pre-gyp package --target_platform=darwin --runtime=electron --target=1.4.10"
  },
  "binary": {
    "module_name": "node_printer",
    "module_path": "./build/{configuration}/electron-v1.4.10-{platform}-{arch}/",
    "package_name": "electron-v1.4.10-{platform}-{arch}.tar.gz",
    "host": "https://github.com/DeekyJay/electron-printer/releases/download/0.0.7"
  },
  "dependencies": {
    "nan": "^2.1.0",
    "node-pre-gyp": "^0.6.13",
    "node-pre-gyp-github": "^1.1.2"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
    "nodeunit": "*"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "main": "./index"
}
